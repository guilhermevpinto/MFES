class Point
instance variables

			-- Coordinates of the point
			private x : [nat];
			private y : [nat];
			
operations

			-- constructor
			public Point: nat * nat ==> Point
      	Point(x_input, y_input) == (x := x_input; y := y_input)
      post x <> nil and y <> nil;
      
      -- setting new x and y parameters of the class
      public setPoint : nat * nat ==> ()
      setPoint(x_set, y_set) ==
    	(
    		x := x_set; y := y_set;
    	)
    	post x <> nil and y <> nil;
    	
    	-- getters of the parameters of the class
      public pure getX : () ==> nat
      getX() ==
      	return x;
      
      public pure getY : () ==> nat
      getY() ==
      	return y;

end Point